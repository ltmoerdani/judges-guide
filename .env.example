# Environment Configuration for Archery Equipment Guide

# Development
NODE_ENV=development
VITE_APP_TITLE="Archery Equipment Master Guide"
VITE_APP_VERSION="1.0.0"

# API Configuration (for future backend integration)
VITE_API_BASE_URL=https://api.archery-guide.com
VITE_API_VERSION=v1

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_SENTRY_DSN=https://xxxxxxxxxx@sentry.io/xxxxxxx

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_ANALYTICS=false

# Performance
VITE_SEARCH_DEBOUNCE_MS=300
VITE_ANIMATION_DURATION_MS=300

# Debug
VITE_DEBUG_MODE=false
VITE_CONSOLE_LOGS=true